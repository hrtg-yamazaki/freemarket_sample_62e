!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title FreemarketSample62e
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  %body
    =render partial: 'shared/header'
    =render partial: 'shared/sell_icon'

    .mypage-guide
      =link_to 'Fmarket', root_path, class: 'guide-link'
      >
      =link_to "#{ yield :current_page }", yield(:current_path), class: 'guide-link guide-link--now'

    .mypage-index
      .mypage-nav
        =render partial: 'shared/nav-bar'
      .mypage-contents
        =render partial: 'shared/content'


    =render partial: 'shared/footer'

